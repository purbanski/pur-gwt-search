<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:p1="urn:import:com.google.gwt.user.cellview.client">

	<g:HTMLPanel width="1050px" height="1050px">
		<g:LayoutPanel width="1050px" height="950px">
			<g:layer left="102px" width="343px" top="17px" height="342px">
				<g:VerticalPanel spacing="10" borderWidth="0">
					<g:Button text="Ouvrir popup de recherche" ui:field="popupButton" width="190px"/>
				</g:VerticalPanel>
			</g:layer>
			<g:layer left="26px" width="982px" top="67px" height="883px">
				<g:VerticalPanel ui:field="panel1" borderWidth="2" width="1050px" height="850px">
					<g:Label text="Les résultats de la recherche" ui:field="labelStock" styleName="gwt-RichTextToolbar"/>
					<p1:DataGrid width="1050px" height="850px" ui:field="dataGrid" title="Les résultats de la recherche"/>
				</g:VerticalPanel>
			</g:layer>
		</g:LayoutPanel>
		
	</g:HTMLPanel>
</ui:UiBinder>
