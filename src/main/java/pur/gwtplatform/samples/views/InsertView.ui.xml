<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<g:HTMLPanel width="750px" height="199px">
			<g:LayoutPanel width="735px" height="194px">
				<g:layer left="153px" width="500px" top="29px" height="196px">
					<g:VerticalPanel width="500px" horizontalAlignment="ALIGN_CENTER" borderWidth="2">
						<g:HorizontalPanel horizontalAlignment="ALIGN_CENTER" width="460px" styleName="gwt-StackPanel">
							<g:Label text="Saisir des données dans le LocalStorage" horizontalAlignment="ALIGN_CENTER" width="487px" styleName="gwt-RichTextToolbar"/>
						</g:HorizontalPanel>
						<g:HorizontalPanel spacing="3" width="460px">
							<g:Label text="Clé de stockage" horizontalAlignment="ALIGN_RIGHT" width="110px" />
							<g:TextBox ui:field="keyStock" />
						</g:HorizontalPanel>
						<g:HorizontalPanel spacing="3" width="460px">
							<g:Label text="Donnée à stoquer" horizontalAlignment="ALIGN_RIGHT" width="110px" />
							<g:TextBox ui:field="dataStock" />
						</g:HorizontalPanel>
						<g:HorizontalPanel spacing="10" width="460px">
							<g:Button ui:field="insertButton" text="Insérer storage" width="102"></g:Button>
							<g:Button ui:field="cancelButton" text="Annuler" width="102"/>
						</g:HorizontalPanel>
					</g:VerticalPanel>
				</g:layer>
			</g:LayoutPanel>
		</g:HTMLPanel>
</ui:UiBinder>
