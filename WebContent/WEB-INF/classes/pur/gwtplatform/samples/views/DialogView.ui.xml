<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:DialogBox modal="true" width="500px" height="245px">
        <g:caption>
            <b>Saisir des données dans le localStorage</b>
        </g:caption>
        <g:HTMLPanel width="460px" height="199px">
			<g:LayoutPanel width="460px" height="180px">
				<g:layer left="0px" width="500px" top="15px" height="76px">
					<g:VerticalPanel width="460px" height="74px">
						<g:HorizontalPanel spacing="3" width="460px">
							<g:Label text="Clé de stockage" horizontalAlignment="ALIGN_RIGHT" width="110px" />
							<g:TextBox ui:field="keyStock" />
						</g:HorizontalPanel>
						<g:HorizontalPanel spacing="3" width="460px">
							<g:Label text="Donnée à stoquer" horizontalAlignment="ALIGN_RIGHT" width="110px" />
							<g:TextBox ui:field="dataStock" />
						</g:HorizontalPanel>
					</g:VerticalPanel>
				</g:layer>
				<g:layer left="0px" width="460px" top="136px" height="44px">
					<g:HorizontalPanel spacing="10" width="460px" horizontalAlignment="ALIGN_CENTER">
							<g:Button ui:field="validerButton" text="Valider" width="102"></g:Button>
							<g:Button ui:field="annulerButton" text="Annuler" width="102"/>
						</g:HorizontalPanel>
				</g:layer>
			</g:LayoutPanel>
		</g:HTMLPanel>
        
    </g:DialogBox>
</ui:UiBinder> 